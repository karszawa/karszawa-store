{
  "version": 2,
  "github": {
    "enabled": false
  },
  "builds": [
    { "src": "package.json", "use": "@now/next" },
    { "src": "public/**", "use": "@now/static" }
  ],
  "routes": [
    { "src": "/items/(?<id>[^/]+)$", "dest": "/items-id?id=$id" },
    { "src": "/transaction/buy", "dest": "/transaction-buy" },
    { "src": "/public/(.*)", "dest": "public/$1" }
  ]
}
